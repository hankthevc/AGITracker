{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd apps/web && npm run build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "npm install --prefix apps/web",
  "cleanUrls": true,
  "trailingSlash": false,
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_API_BASE_URL": ""
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
