# Normalize line endings
* text=auto eol=lf

# Specific file types
*.py text eol=lf
*.ts text eol=lf
*.tsx text eol=lf
*.js text eol=lf
*.jsx text eol=lf
*.json text eol=lf
*.md text eol=lf
*.yml text eol=lf
*.yaml text eol=lf
*.sh text eol=lf

# Binary files
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.woff binary
*.woff2 binary
*.ttf binary
*.eot binary

# Linguist overrides
*.md linguist-documentation
docs/* linguist-documentation
*.sql linguist-detectable=true
infra/migrations/* linguist-generated=false
packages/scoring/* linguist-vendored=false

# Git diff
*.lock linguist-generated=true
package-lock.json linguist-generated=true merge=ours
*.tsbuildinfo linguist-generated=true

